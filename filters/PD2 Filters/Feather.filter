//-¦ Other Basic Items ¦-------------------------------------------------------------------------------------------------
// +-------------------+
// Hidden: Keys, Scrolls - level 20
ItemDisplay[key CLVL>19 !(ILVL=28 OR ILVL=64 OR ILVL=81)]: {%WHITE%Key}			// except in Cow Levels
ItemDisplay[isc CLVL>19]: {%WHITE%Scroll of Identify}
ItemDisplay[tsc CLVL>19]: {%WHITE%Scroll of Town Portal}

// Modification
ItemDisplay[wss]:	%RED%Worldstone Shard%CONTINUE%								// sets color for Worldstone Shard
ItemDisplay[lbox]:	%RED%Larzuk's Puzzlebox%CONTINUE%							// sets color for Larzuk's Puzzlebox
ItemDisplay[QTY>1 !(WEAPON OR aqv OR cqv OR tbk OR ibk OR key OR RUNE>0)]: %NAME% %WHITE%×%QTY%%CONTINUE%	// appends quantity

